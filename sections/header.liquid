<dev>
  <ul>
    {% for link in linklists["main-menu"].links %}
      <li>
        {% if link.links != blank %}
         <a href="{{ link.url }}">{{ link.title }}</a>
          <ul>
            {% for child in link.links %}
              <li>
                <a href="{{ child.url }}">{{ child.title }}</a>
              </li>
            {% endfor %}
          </ul>
        {% else %}
          <a href="{{ link.url }}">{{ link.title }}</a>
        {% endif %}
      </li>
      {% endfor %}
  </ul>
</dev>